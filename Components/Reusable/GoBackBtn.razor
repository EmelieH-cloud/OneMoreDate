@inject NavigationManager Navigation


<div class="navigate-btn-container">
    <button @onclick="GoBack" class="back-btn animate__animated animate__fadeIn">
        <svg xmlns="http://www.w3.org/2000/svg"  fill="currentColor" class="bi bi-chevron-left back-button-svg" viewBox="0 0 16 16">
            <path fill-rule="evenodd" d="M11.354 1.646a.5.5 0 0 1 0 .708L5.707 8l5.647 5.646a.5.5 0 0 1-.708.708l-6-6a.5.5 0 0 1 0-.708l6-6a.5.5 0 0 1 .708 0" />
        </svg>
    </button>
</div>


@code
{


    [Parameter]
    public string BackUrl { get; set; }


    private void GoBack()
    {
        Navigation.NavigateTo($"/{BackUrl}");
    }

}
